{
  "version": 2,
  "builds": [
    {
      "src": "feature-api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "feature-frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/predict_price", "dest": "feature-api/app.py" },
    { "src": "/(.*)", "dest": "feature-frontend/$1" }
  ]
}
